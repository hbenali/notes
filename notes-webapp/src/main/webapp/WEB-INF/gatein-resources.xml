<gatein-resources
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.exoplatform.org/xml/ns/gatein_resources_1_5 http://www.exoplatform.org/xml/ns/gatein_resources_1_5"
  xmlns="http://www.exoplatform.org/xml/ns/gatein_resources_1_5">

  <portal-skin>
    <skin-name>Enterprise</skin-name>
    <skin-module>Notes</skin-module>
    <css-path>/skin/css/notes/notes.css</css-path>
    <filtered>true</filtered>
  </portal-skin>

  <portlet-skin>
    <application-name>notes</application-name>
    <portlet-name>Notes</portlet-name>
    <skin-name>Enterprise</skin-name>
    <additional-module>Notes</additional-module>
  </portlet-skin>

  <portlet-skin>
    <application-name>notes</application-name>
    <portlet-name>NotesEditor</portlet-name>
    <skin-name>Enterprise</skin-name>
    <additional-module>Notes</additional-module>
  </portlet-skin>

  <portlet-skin>
    <application-name>notes</application-name>
    <portlet-name>NotePageView</portlet-name>
    <skin-name>Enterprise</skin-name>
    <additional-module>Notes</additional-module>
  </portlet-skin>

  <portlet>
    <name>Notes</name>
    <module>
      <load-group>NotesGRP</load-group>
      <script>
        <minify>false</minify>
        <path>/javascript/notes.bundle.js</path>
      </script>
      <depends>
        <module>html2canvas</module>
      </depends>
      <depends>
        <module>jspdf</module>
      </depends>
      <depends>
        <module>commonVueComponents</module>
      </depends>
      <depends>
        <module>extensionRegistry</module>
      </depends>
      <depends>
        <module>vue</module>
      </depends>
      <depends>
        <module>vuetify</module>
      </depends>
      <depends>
        <module>eXoVueI18n</module>
      </depends>
      <depends>
        <module>versionHistoryDrawer</module>
      </depends>
      <depends>
        <module>jquery</module>
        <as>$</as>
      </depends>
    </module>
  </portlet>

  <portlet>
    <name>NotesEditor</name>
    <module>
      <load-group>NotesEditorGRP</load-group>
      <script>
        <minify>false</minify>
        <path>/javascript/notesEditor.bundle.js</path>
      </script>
      <depends>
        <module>commonVueComponents</module>
      </depends>
      <depends>
        <module>extensionRegistry</module>
      </depends>
      <depends>
        <module>vue</module>
      </depends>
      <depends>
        <module>vuetify</module>
      </depends>
      <depends>
        <module>commons-editor</module>
        <as>editor</as>
      </depends>
      <depends>
        <module>eXoVueI18n</module>
      </depends>
      <depends>
        <module>jquery</module>
        <as>$</as>
      </depends>
    </module>
  </portlet>

  <portlet>
    <name>NotePageView</name>
    <module>
      <script>
        <minify>false</minify>
        <path>/javascript/notePageView.bundle.js</path>
      </script>
      <depends>
        <module>commonVueComponents</module>
      </depends>
      <depends>
        <module>vue</module>
      </depends>
      <depends>
        <module>vuetify</module>
      </depends>
      <depends>
        <module>eXoVueI18n</module>
      </depends>
      <depends>
        <module>extensionRegistry</module>
      </depends>
    </module>
  </portlet>

  <module>
    <name>NotesActivityStreamExtension</name>
    <load-group>ActivityStreamGRP</load-group>
    <script>
      <path>/javascript/notesActivityStreamExtension.bundle.js</path>
    </script>
    <depends>
      <module>extensionRegistry</module>
    </depends>
    <depends>
      <module>eXoVueI18n</module>
    </depends>
    <depends>
      <module>vue</module>
    </depends>
    <depends>
      <module>commonVueComponents</module>
    </depends>
  </module>

  <module>
    <name>NotesFavoriteDrawerExtension</name>
    <load-group>FavoriteDrawerGRP</load-group>
    <script>
      <path>/javascript/notesFavoriteDrawerExtension.bundle.js</path>
    </script>
    <depends>
      <module>extensionRegistry</module>
    </depends>
    <depends>
      <module>eXoVueI18n</module>
    </depends>
    <depends>
      <module>vue</module>
    </depends>
    <depends>
      <module>commonVueComponents</module>
    </depends>
  </module>

  <module>
    <name>wikiPageContent</name>
    <script>
      <minify>false</minify>
      <path>/javascript/pageContent.bundle.js</path>
    </script>
    <depends>
      <module>vue</module>
    </depends>
    <depends>
      <module>NotesActivityStreamExtension</module>
    </depends>
    <depends>
      <module>vueCustomElement</module>
    </depends>
  </module>

  <module>
    <name>wikiSearchCard</name>
    <script>
      <minify>false</minify>
      <path>/javascript/wikiSearchCard.bundle.js</path>
    </script>
    <depends>
      <module>commonVueComponents</module>
    </depends>
    <depends>
      <module>vue</module>
    </depends>
    <depends>
      <module>eXoVueI18n</module>
    </depends>
    <depends>
      <module>vuetify</module>
    </depends>
    <depends>
      <module>jquery</module>
      <as>$</as>
    </depends>
    <depends>
      <module>extensionRegistry</module>
    </depends>
  </module>

  <module>
    <name>vueCustomElement</name>
    <script>
      <path>/javascript/lib/vue-custom-element.min.js</path>
    </script>
    <depends>
      <module>vue</module>
    </depends>
  </module>

  <module>
    <name>engagementCenterActionsNoteExtensions</name>
    <load-group>engagement-center-user-actions</load-group>
    <script>
      <path>/javascript/engagementCenterExtensions.bundle.js</path>
    </script>
    <depends>
      <module>vue</module>
    </depends>
    <depends>
      <module>vuetify</module>
    </depends>
    <depends>
      <module>eXoVueI18n</module>
    </depends>
    <depends>
      <module>extensionRegistry</module>
    </depends>
  </module>

  <module>
    <name>engagementCenterConnectorEventsNotesExtensions</name>
    <load-group>engagement-center-connector-event-extensions</load-group>
    <script>
      <path>/javascript/connectorEventExtensions.bundle.js</path>
    </script>
    <depends>
      <module>vue</module>
    </depends>
    <depends>
      <module>vuetify</module>
    </depends>
    <depends>
      <module>eXoVueI18n</module>
    </depends>
    <depends>
      <module>extensionRegistry</module>
    </depends>
    <depends>
      <module>commonVueComponents</module>
    </depends>
  </module>
  
</gatein-resources>
